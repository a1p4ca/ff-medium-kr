{
    "manifest_version": 2,
    "name": "Medium-KR",
    "version": "1.0",
    "description": "Support korean font in Medium.",
    "icons": {
        "48": ""
    },
    "content_scripts": [
        {
            "matches": [
                "*://medium.com/*"
            ],
            "js": [
                "src/medium-kr.js"
            ]
        }
    ],
    "options_ui": {
        "page": "src/options/options.html"
    },
    "permissions": ["storage"]
}
